CC=g++
CFLAGS=-Wall
LDFLAGS=-lsfml-graphics -lsfml-window -lsfml-system -Wall
EXEC=generate
OBJ=main.o mapGenrator.o

%.o : src/%.cpp src/headers/%.h
	$(CC) -c src/$*.cpp $(CFLAGS)

main.o :
	$(CC) -c src/main.cpp $(CFLAGS)  

generate : $(OBJ)
	$(CC) -o $@ $^ $(LDFLAGS) 
	rm -rf *.o

clean:
	rm -rf *.o
